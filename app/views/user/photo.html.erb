<% provide(:title, "#{@user.name ? @user.name + ' | Photo' : 'Photo'}") %>
<%= render 'user/layout_profile' %>

<section class="container-fluid bh_main_content" style="margin-top:0">
  <div class="photo-container">
    <div class="row">
      <h2 class="text-center">Photos(<%= @images.count %>) </h2>
      <div class="grid row">
        <% @images.each do |image| %>
          <%= render "images/image", image: image %>
        <% end %>
      </div>
      <%= will_paginate(@images, class: "hide") %>
    </div>
  </div>
</section>
<script type="text/javascript">
  $(document).ready(function(){
    $(".photo").addClass("active");
  });
</script>